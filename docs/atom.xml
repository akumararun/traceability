---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>MantisBT Traceability Plugin</title>
	<link href="https://traceability.github.io/atom.xml" rel="self"/>
	<link href="https://traceability.github.io/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://traceability.github.io/</id>
	<author>
		<name>STRUCT-IT</name>
		<email>contact@struct-it.fr</email>
	</author>
	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://traceability.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://traceability.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>